    device = "/dev/disk/by-uuid/3c8fea09-e60a-43fd-8383-bd1d421ab3d2";
    fsType = "ext4";
  };

  boot.initrd.luks.devices."luks-5d64c630-9733-456a-b11a-54f1df12d91b".device =
