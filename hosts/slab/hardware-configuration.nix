  fileSystems."/boot" = {
    device = "/dev/disk/by-uuid/8524-62C2";
    fsType = "vfat";
    options = [
      "fmask=0077"
      "dmask=0077"
    ];
  };

  swapDevices = [
    { device = "/dev/disk/by-uuid/b344af0b-8680-41be-ab1e-dbf0de7faecc"; }
